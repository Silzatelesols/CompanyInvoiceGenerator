# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# AWS S3 Configuration (Images - Private)
VITE_AWS_S3_BUCKET=your-image-bucket
VITE_AWS_REGION=ap-south-1
VITE_AWS_ACCESS_KEY_ID=your_access_key_id
VITE_AWS_SECRET_ACCESS_KEY=your_secret_access_key

# AWS S3 Configuration (PDFs - Public)
VITE_AWS_S3_PDF_BUCKET=your-pdf-bucket

# Email Integration
VITE_EMAIL_API_ENDPOINT=https://your-api-gateway-endpoint.execute-api.region.amazonaws.com/prod

# PDF Generation
VITE_USE_PDFMAKE=true

